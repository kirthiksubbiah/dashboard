  "dataSources": {
    "ds_mDhHHhGn": {
      "type": "ds.search",
      "options": {
        "query": "index=main sourcetype=access_combined earliest=-24h@h latest=now\n| eval sc = tonumber(status)\n| eval status_class = case(sc>=200 AND sc<300,\"2xx\", sc>=400 AND sc<500,\"4xx\", sc>=500 AND sc<600,\"5xx\", 1==1,\"other\")\n| search status_class=\"2xx\" OR status_class=\"4xx\" OR status_class=\"5xx\"\n| stats count by status_class"
      },
      "name": "Status counts - Column chart search"
    },
    "ds_bwxdjGQe": {
      "type": "ds.search",
      "options": {
        "query": "index=main sourcetype=access_combined earliest=-24h@h latest=now\n| eval sc=tonumber(status)\n| where sc>=200 AND sc<300\n| stats count as \"2xx_count\""
      },
      "name": "2xx responses - Single value search"
    },
    "ds_EP7cQ3Sy": {
      "type": "ds.search",
      "options": {
        "query": "index=main sourcetype=access_combined earliest=-24h@h latest=now\n| eval sc=tonumber(status)\n| where sc>=400 AND sc<500\n| stats count as \"4xx_count\""
      },
      "name": "4xx responses - Single value search"
    },
    "ds_7KxpV41K": {
      "type": "ds.search",
      "options": {
        "query": "index=main sourcetype=access_combined earliest=-24h@h latest=now\n| eval sc=tonumber(status)\n| where sc>=500 AND sc<600\n| stats count as \"5xx_count\""
      },
      "name": "5xx responses - Single value search"
    },
    "ds_ic1cEujj": {
      "type": "ds.search",
      "options": {
        "query": "index=main sourcetype=access_combined earliest=-24h@h latest=now\n| rex field=_raw \"^(?<clientip>\\S+) \\S+ \\S+ \\[(?<time>[^\\]]+)\\] \\\"(?<method>\\S+)\\s+(?<uri>\\S+)(?:\\s+\\S+)?\\\" (?<status>\\d{3}) (?<bytes>\\d+)(?:\\s+\\\"(?<referrer>[^\\\"]*)\\\" \\\"(?<useragent>[^\\\"]*)\\\")?\"\n| eval sc = tonumber(status)\n| eval status_class = case(sc>=200 AND sc<300,\"2xx\", sc>=400 AND sc<500,\"4xx\", sc>=500 AND sc<600,\"5xx\", 1==1,\"other\")\n| where ( status_class = \"$selected_status$\" )\n| table _time, clientip, method, uri, status, bytes, useragent\n| sort - _time"
      },
      "name": "Details - Table search"
    }
