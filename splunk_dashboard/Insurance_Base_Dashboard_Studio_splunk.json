  "dataSources": {
    "ds_9uypWSwv": {
      "type": "ds.search",
      "options": {
        "query": "index=main sourcetype=matric_log domain=Application\n| eval _time = strptime(timestamp, \"%Y-%m-%d %H:%M:%S\")\n| sort 0 - _time\n| stats latest(availability) AS Availability latest(latency_ms) AS Latency_ms latest(error_rate) AS ErrorRate latest(saturation_pct) AS Saturation BY name\n| eval Availability = round(Availability,0), Latency_ms = round(Latency_ms,0), Saturation = round(Saturation,0)\n| rename name AS \"Application Name\", Latency_ms AS Latency\n| table \"Application Name\" Availability Latency ErrorRate Saturation",
        "queryParameters": {
          "earliest": "0",
          "sampleRatio": 1
        }
      },
      "name": "Application Health - APM - Table search"
    },
    "ds_w7ZAI36t": {
      "type": "ds.search",
      "options": {
        "query": "index=main sourcetype=matric_log domain=Infrastructure\n| eval _time = strptime(timestamp, \"%Y-%m-%d %H:%M:%S\")\n| sort 0 - _time\n| stats latest(availability) AS Availability latest(latency_ms) AS Latency_ms latest(error_rate) AS ErrorRate latest(saturation_pct) AS Saturation BY name\n| eval Availability = round(Availability,0), Latency_ms = round(Latency_ms,0), Saturation = round(Saturation,0)\n| rename name AS \"Infrastructure Component\", Latency_ms AS Latency\n| table \"Infrastructure Component\" Availability Latency ErrorRate Saturation",
