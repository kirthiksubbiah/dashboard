  "dataSources": {
    "ds_BnAiaZ4X": {
      "type": "ds.search",
      "options": {
        "query": "sourcetype=\"enrollments.csv\"\n| stats sum(count) AS total\n| eval total = tostring(total, \"commas\")",
        "queryParameters": {
          "earliest": "0",
          "sampleRatio": 1
        }
      },
      "name": "Total     Enrollments - Single value search"
    },
    "ds_ecnYvlI2": {
      "type": "ds.search",
      "options": {
        "query": "sourcetype=\"enrollments.csv\"\n| search statusCode=\"Enrollment In-Progress\" OR status=\"in-progress\"\n| stats sum(count) AS total\n| eval total = tostring(total, \"commas\")",
        "queryParameters": {
          "earliest": "0",
          "sampleRatio": 1
        }
      },
      "name": "In-progress enrollments - Single value search"
    },
    "ds_VAMlGVhI": {
      "type": "ds.search",
      "options": {
        "query": "sourcetype=\"enrollments.csv\"\n| search statusCode=\"Enrollment Completed\" OR status=\"completed\"\n| stats sum(count) AS total\n| eval total = tostring(total, \"commas\")",
        "queryParameters": {
          "earliest": "0",
          "sampleRatio": 1
        }
      },
      "name": "Completed Enrollments - Single value search"
    },
    "ds_Rj7NISKK": {
      "type": "ds.search",
      "options": {
        "query": "sourcetype=\"enrollments.csv\"\n| search statusCode=\"Enrollment Failed\" OR statusCode=\"PostOrderProcessingError\" OR statusCode=\"PreOrderProcessingError\"\n| stats sum(count) AS total\n| eval total = tostring(total, \"commas\")",
        "queryParameters": {
          "earliest": "0",
          "sampleRatio": 1
        }
      },
      "name": "Failed Enrollments - Single value search"
    },
    "ds_2ULctwcE": {
      "type": "ds.search",
      "options": {
        "query": "sourcetype=\"enrollments.csv\"\n| search status=\"cancelled\" OR statusCode=\"Enrollment Cancelled\"\n| stats sum(count) AS total\n| eval total = tostring(total, \"commas\")",
        "queryParameters": {
          "earliest": "0",
          "sampleRatio": 1
        }
      },
      "name": "Cancelled Enrollments - Single value search"
    },
    "ds_Hn7sryK6": {
      "type": "ds.search",
      "options": {
        "query": "sourcetype=\"enrollments.csv\"\n| search status=\"rejected\" OR statusCode=\"Enrollment Rejected\"\n| stats sum(count) AS total\n| eval total = tostring(total, \"commas\")",
        "queryParameters": {
          "earliest": "0",
          "sampleRatio": 1
        }
      },
      "name": "Rejected Enrollments - Single value search"
    },
    "ds_yXyJenVK": {
      "type": "ds.search",
      "options": {
        "query": "sourcetype=\"enrollments.csv\"\n| table statusCode activityId subStatusCode EnrollmentID ProcessID",
        "queryParameters": {
          "earliest": "0",
          "sampleRatio": 1
        }
      },
      "name": "Table search"
    },
    "ds_j0wiZooa": {
      "type": "ds.search",
      "options": {
        "query": "sourcetype=\"enrollments.csv\" ProcessID=\"$ProcessID$\" activityId=\"$activityId$\"\n| table ProcessID EnrollmentID status statusCode subStatusCode reasonDescription",
        "queryParameters": {
          "earliest": "0",
          "latest": "",
          "sampleRatio": 1
        }
      },
      "name": "Failed Enrollment Error Details - Table search"
    }
