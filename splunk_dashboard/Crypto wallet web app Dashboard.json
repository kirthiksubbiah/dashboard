    "dataSources": {
        "ds_66J9cr2N": {
            "name": "Total wallets created - Single value search",
            "options": {
                "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" \"Created wallet\"\n| stats count as \"Total Wallets Created\"",
                "queryParameters": {
                    "earliest": "-1d@d",
                    "latest": "@d",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_738zvhR3": {
            "name": "Total Deposits - Single value search",
            "options": {
                "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" \n| where like(_raw, \"Deposited%\")\n| stats count as total_deposits",
                "queryParameters": {
                    "earliest": "-1d@d",
                    "latest": "@d",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_7wey6OK1": {
            "name": "Total events - Single value search",
            "options": {
                "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" \n| stats count AS \"Total Events\"",
                "queryParameters": {
                    "earliest": "-1d@d",
                    "latest": "@d",
                    "sampleRatio": 1
                },
                "refresh": "30s",
                "refreshType": "delay"
            },
            "type": "ds.search"
        },
        "ds_8w1Qjcr8": {
            "name": "Failed operations - Single value search",
            "options": {
                "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" \"not found\"\n| stats count AS \"Failed Operations\"",
                "queryParameters": {
                    "earliest": "-1d@d",
                    "latest": "@d",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_CGEBw9x3": {
            "name": "Number of withdrawals per currency - Column chart search",
            "options": {
                "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\"  \"Withdrew\"\n| rex field=_raw \"(?<currency>BTC|GBP|USD)\"\n| stats count by currency",
                "queryParameters": {
                    "earliest": "-1d@d",
                    "latest": "@d",
                    "sampleRatio": 1
                },
                "refresh": "30s",
                "refreshType": "delay"
            },
            "type": "ds.search"
        },
        "ds_ErzUBhKL": {
            "name": "Outgoing transfers per currency - Column chart search",
            "options": {
                "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\"  \"Transferred\"\n| rex field=_raw \"(?<currency>BTC|GBP|USD)\"\n| stats count by currency",
                "queryParameters": {
                    "earliest": "-1d@d",
                    "latest": "@d",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_NyeArIe8": {
            "name": "Total Withdrawals - Single value search",
            "options": {
                "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" \n| where like(_raw, \"Withdrew%\")\n| stats count as total_withdrawals",
                "queryParameters": {
                    "earliest": "-1d@d",
                    "latest": "@d",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_QOYDvPNn": {
            "name": "Total Errors - Single value search",
            "options": {
                "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\"  \"not found\"\n| stats count as \"Total Errors\"",
                "queryParameters": {
                    "earliest": "-1d@d",
                    "latest": "@d",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_TFtiWevO": {
            "name": "Wallet Transfer Source Map - Cluster map search",
            "options": {
                "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" \"Transferred\"\n| rex field=_raw \"Transferred\\s+(?<amount>\\d+)\\s+(?<currency>[A-Z]{3})\\s+from wallet\\s+(?<from_wallet>[0-9a-f-]+)\\s+at location\\s+(?<from_location>[^,]+),\\s+latitude\\s+(?<from_lat>[-\\d\\.]+),\\s+longitude\\s+(?<from_lon>[-\\d\\.]+)\\s+to\\s+(?<to_amount>\\d+)\\s+(?<to_currency>[A-Z]{3})\\s+in wallet\\s+(?<to_wallet>[0-9a-f-]+)\\s+at location\\s+(?<to_location>[^,]+),\\s+latitude\\s+(?<to_lat>[-\\d\\.]+),\\s+longitude\\s+(?<to_lon>[-\\d\\.]+)\"\n| eval amount=tonumber(amount), to_amount=tonumber(to_amount)\n| geostats latfield=from_lat longfield=from_lon count by currency",
                "queryParameters": {
                    "earliest": "-1d@d",
                    "latest": "@d",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_gfq5Gkdj": {
            "name": "Deposits _ Withdrawals summary - Table search",
            "options": {
                "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\"  (Deposited OR Withdrew)\n| rex field=_raw \"(?<action>Deposited|Withdrew)\\s+(?<amount>\\d+)\\s+(?<currency>[A-Z]{3})\\s+(?:to|from)\\s+wallet\\s+(?<wallet>[0-9a-f-]+)\\s+at location\\s+(?<location>[^,]+)\\s*,\\s*latitude\\s+(?<lat>[-\\d\\.]+),\\s*longitude\\s+(?<lon>[-\\d\\.]+)\"\n| eval amount=tonumber(amount)\n| stats count sum(amount) AS total_amount by action currency",
                "queryParameters": {
                    "earliest": "-1d@d",
                    "latest": "@d",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_hVbAszmD": {
            "name": "Number of deposits per currency - Column chart search",
            "options": {
                "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\"  \"Deposited\"\n| rex field=_raw \"(?<currency>BTC|GBP|USD)\"\n| stats count by currency",
                "queryParameters": {
                    "earliest": "-1d@d",
                    "latest": "@d",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_jyMcnZDv": {
            "name": "Total Transfers - Single value search",
            "options": {
                "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" \"Transferred\"\n| stats count AS \"Total Transfers\"",
                "queryParameters": {
                    "earliest": "-1d@d",
                    "latest": "@d",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_kFX1woO8": {
            "name": "event rate by hour - Line chart search",
            "options": {
                "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" \n| timechart span=1h count AS \"Event Count\"",
                "queryParameters": {
                    "earliest": "-1d@d",
                    "latest": "@d",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_m9tzocR9": {
            "name": "Number of transactions by currency - Column chart search",
            "options": {
                "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" \n\"Deposited\" OR \"Withdrew\" OR \"Transferred\"\n| rex field=_raw \"(?<currency>BTC|GBP|USD)\"\n| stats count by currency",
                "queryParameters": {
                    "earliest": "-1d@d",
                    "latest": "@d",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_mVdu4ThQ": {
            "name": "Error _ - Single value search",
            "options": {
                "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" \n| eval is_error=if(like(_raw,\"%not found%\"),1,0)\n| stats count as total_events sum(is_error) as total_errors\n| eval error_percentage = round((total_errors/total_events)*100,2)\n| fields error_percentage",
                "queryParameters": {
                    "earliest": "-1d@d",
                    "latest": "@d",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_t6Wu0hJa": {
            "name": "Total Transactions - Single value search",
            "options": {
                "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" \"Deposited\" OR \"Withdrew\" OR \"Transferred\"\n| stats count as \"Total Transactions\"",
                "queryParameters": {
                    "earliest": "-1d@d",
                    "latest": "@d",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_tmKQqMNi": {
            "name": "Details for _details_ - Table search",
            "options": {
                "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\"\n            | eval category=case(\n            match(_raw,\"Created wallet\"),\"wallets_created\",\n            match(_raw,\"Deleted wallet\"),\"wallets_deleted\",\n            match(_raw,\"Deposited\"),\"deposits\",\n            match(_raw,\"Withdrew\"),\"withdrawals\",\n            match(_raw,\"Transferred\"),\"transfers\",\n            like(_raw,\"%not found%\"),\"errors\",\n            true(),\"events\")\n            | search category=$details$\n            | table _time _raw",
                "queryParameters": {
                    "earliest": "-1d@d",
                    "latest": "@d",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_tsEKvZJ5": {
            "name": "Total Wallets Deleted - Single value search",
            "options": {
                "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\"  \"Deleted wallet\"\n| stats count as \"Total Wallets Deleted\"",
                "queryParameters": {
                    "earliest": "-1d@d",
                    "latest": "@d",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        }
    },
