      "widgets": [
        {
          "id": "498569457",
          "title": "Error SLO Compliance (%)",
          "layout": {
            "column": 1,
            "row": 1,
            "width": 4,
            "height": 3
          },
          "linkedEntityGuids": [],
          "visualization": {
            "id": "viz.billboard"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "nrqlQueries": [
              {
                "accountIds": [
                  7291193
                ],
                "query": "FROM Span \r\nSELECT 100 - percentage(count(*), WHERE error = true) AS 'Error SLO Compliance (%)'\r\n"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
        },
        {
          "id": "498569458",
          "title": "Latency (95th %)",
          "layout": {
            "column": 5,
            "row": 1,
            "width": 4,
            "height": 3
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.billboard"
          },
          "rawConfiguration": {
            "nrqlQueries": [
              {
                "accountIds": [
                  7291193
                ],
                "query": "FROM Span SELECT percentile(duration.ms,95) AS 'P95 Latency (ms)' WHERE service.name IN ({{service}})"
              }
            ]
          }
        },
        {
          "id": "498569460",
