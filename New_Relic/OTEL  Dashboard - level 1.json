    {
      "guid": "NzI5MTE5M3xWSVp8REFTSEJPQVJEfDQ0ODQ5MzA5",
      "name": "Key Matrics",
      "description": null,
      "widgets": [
        {
          "id": "498583916",
          "title": "Error count per service",
          "layout": {
            "column": 1,
            "row": 1,
            "width": 4,
            "height": 3
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.line"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "markers": {
              "displayedTypes": {
                "criticalViolations": false,
                "deployments": true,
                "relatedDeployments": true,
                "warningViolations": false
              }
            },
            "nrqlQueries": [
              {
                "accountIds": [
                  7291193
                ],
                "query": "FROM Span\r\nSELECT count(*) AS 'Error Count'\r\nWHERE http.status_code >= 500 AND service.name IN ({{service}})\r\nFACET service.name\r\nTIMESERIES AUTO\r\n"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            },
            "thresholds": {
              "isLabelVisible": true
            },
            "yAxisLeft": {
              "zero": true
            },
            "yAxisRight": {
              "zero": true
            }
          }
        },
        {
          "id": "498583982",
          "title": "Error rate per service",
          "layout": {
            "column": 5,
            "row": 1,
            "width": 4,
            "height": 3
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.line"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "markers": {
              "displayedTypes": {
                "criticalViolations": false,
                "deployments": true,
                "relatedDeployments": true,
                "warningViolations": false
              }
            },
            "nrqlQueries": [
              {
                "accountIds": [
                  7291193
                ],
                "query": "FROM Span\r\nSELECT \r\n  100 * filter(count(*), WHERE http.status_code >= 500) / count(*) AS 'Error Rate (%)'\r\nWHERE service.name IN ({{service}})\r\nTIMESERIES AUTO\r\n"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            },
            "thresholds": {
              "isLabelVisible": true
            },
            "yAxisLeft": {
              "zero": true
            },
            "yAxisRight": {
              "zero": true
            }
          }
        },
        {
          "id": "498583985",
          "title": "Error per trace",
          "layout": {
            "column": 9,
            "row": 1,
            "width": 4,
            "height": 3
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.bar"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "nrqlQueries": [
              {
                "accountIds": [
                  7291193
                ],
                "query": "FROM Span\r\nSELECT \r\n  filter(count(*), WHERE http.status_code >= 500) AS 'Error Spans',\r\n  count(*) AS 'Total Spans',\r\n  100 * filter(count(*), WHERE http.status_code >= 500) / count(*) AS 'Error Rate (%)'\r\nWHERE service.name IN ({{service}})\r\nFACET trace.id\r\nLIMIT 100\r\n"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
        },
        {
          "id": "498583989",
          "title": "Trace count per service",
          "layout": {
            "column": 1,
            "row": 4,
            "width": 4,
            "height": 3
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.pie"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": true
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountIds": [
                  7291193
                ],
                "query": "FROM Span\r\nSELECT uniqueCount(trace.id) AS 'Trace Count'\r\nWHERE service.name IN ({{service}})\r\nFACET service.name\r\n"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
        },
        {
          "id": "498584016",
          "title": "Duration in ms Per Service",
          "layout": {
