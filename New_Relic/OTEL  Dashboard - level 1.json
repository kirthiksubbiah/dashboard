              {
                "accountIds": [
                  7291193
                ],
                "query": "FROM Span\r\nSELECT average(duration.ms) AS 'Avg Duration (ms)'\r\nWHERE service.name IN ({{service}})\r\n"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
        },
        {
