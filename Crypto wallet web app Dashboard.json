    "ds_r9lDnRdv": {
      "type": "ds.search",
      "options": {
        "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" earliest=-24h latest=now \"Transferred\"\n| stats count AS \"Total Transfers\"",
