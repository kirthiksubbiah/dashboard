      "options": {
        "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" earliest=-24h latest=now \"Deposited\"\n| rex field=_raw \"(?<currency>BTC|GBP|USD)\"\n| stats count by currency",
