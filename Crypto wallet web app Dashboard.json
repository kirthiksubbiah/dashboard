    },
    "ds_d0yX5MOA": {
      "type": "ds.search",
      "options": {
        "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" earliest=-24h latest=now \"Created wallet\"\n| stats count as \"Total Wallets Created\"",
