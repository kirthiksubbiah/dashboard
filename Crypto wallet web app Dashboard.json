    "ds_JrkV8aap": {
      "type": "ds.search",
      "options": {
        "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" earliest=-24h latest=now \"Deposited\" OR \"Withdrew\" OR \"Transferred\"\n| stats count as \"Total Transactions\"",
        "queryParameters": {
          "earliest": "-24h@h",
          "latest": "now",
          "sampleRatio": 1
        }
      },
