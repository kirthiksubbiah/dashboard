        }
      },
      "name": "Total Transfers - Single value search"
    },
    "ds_fTnpVWqa": {
      "type": "ds.search",
      "options": {
        "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" earliest=-24h latest=now\n| where like(_raw, \"Deposited%\")\n| stats count as total_deposits",
        "queryParameters": {
          "earliest": "-24h@h",
          "latest": "now",
          "sampleRatio": 1
        }
      },
      "name": "Total Deposits - Single value search"
    },
    "ds_ZXPEYgGH": {
      "type": "ds.search",
      "options": {
        "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" earliest=-24h latest=now\n| where like(_raw, \"Withdrew%\")\n| stats count as total_withdrawals",
        "queryParameters": {
          "earliest": "-24h@h",
          "latest": "now",
          "sampleRatio": 1
        }
      },
      "name": "Total Withdrawals - Single value search"
    },
    "ds_i3RxYZhk": {
      "type": "ds.search",
      "options": {
        "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" earliest=-24h latest=now \"not found\"\n| stats count AS \"Failed Operations\"",
        "queryParameters": {
          "earliest": "-24h@h",
          "latest": "now",
          "sampleRatio": 1
        }
      },
      "name": "Failed operations - Single value search"
    },
    "ds_ZttZqLbw": {
      "type": "ds.search",
      "options": {
        "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" earliest=-24h latest=now \"Deleted wallet\"\n| stats count as \"Total Wallets Deleted\"",
        "queryParameters": {
          "earliest": "-24h@h",
          "latest": "now",
          "sampleRatio": 1
        }
      },
      "name": "Total Wallets Deleted - Single value search"
    },
    "ds_5LcoHdVR": {
      "type": "ds.search",
      "options": {
        "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" earliest=-24h latest=now\n| eval is_error=if(like(_raw,\"%not found%\"),1,0)\n| stats count as total_events sum(is_error) as total_errors\n| eval error_percentage = round((total_errors/total_events)*100,2)\n| fields error_percentage",
        "queryParameters": {
          "earliest": "-24h@h",
          "latest": "now",
          "sampleRatio": 1
        }
      },
      "name": "Error _ - Single value search"
    },
    "ds_a2lIJRzs": {
      "type": "ds.search",
      "options": {
        "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" earliest=-24h latest=now \"not found\"\n| stats count as \"Total Errors\"",
        "queryParameters": {
          "earliest": "-24h@h",
          "latest": "now",
          "sampleRatio": 1
        }
      },
      "name": "Total Errors - Single value search"
    },
    "ds_DL2yiAe9": {
      "type": "ds.search",
      "options": {
        "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" earliest=-24h latest=now\n            | eval category=case(\n            match(_raw,\"Created wallet\"),\"wallets_created\",\n            match(_raw,\"Deleted wallet\"),\"wallets_deleted\",\n            match(_raw,\"Deposited\"),\"deposits\",\n            match(_raw,\"Withdrew\"),\"withdrawals\",\n            match(_raw,\"Transferred\"),\"transfers\",\n            like(_raw,\"%not found%\"),\"errors\",\n            true(),\"events\")\n            | search category=$details$\n            | table _time _raw",
        "queryParameters": {
          "earliest": "-24h@h",
          "latest": "now",
          "sampleRatio": 1
        }
      },
      "name": "Details for _details_ - Table search"
    },
    "ds_jW0Hp7xL": {
      "type": "ds.search",
      "options": {
        "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" earliest=-24h latest=now\n| timechart span=1h count AS \"Event Count\"",
        "queryParameters": {
          "earliest": "-24h@h",
          "latest": "now",
          "sampleRatio": 1
        }
      },
      "name": "event rate by hour - Line chart search"
    },
    "ds_gXb2lBlh": {
      "type": "ds.search",
      "options": {
        "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" earliest=-24h latest=now\n\"Deposited\" OR \"Withdrew\" OR \"Transferred\"\n| rex field=_raw \"(?<currency>BTC|GBP|USD)\"\n| stats count by currency",
        "queryParameters": {
          "earliest": "-24h@h",
          "latest": "now",
          "sampleRatio": 1
        }
      },
