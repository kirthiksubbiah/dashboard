    "ds_YEZccnRc": {
      "type": "ds.search",
      "options": {
        "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" earliest=-24h latest=now \"Transferred\"\n| rex field=_raw \"Transferred\\s+(?<amount>\\d+)\\s+(?<currency>[A-Z]{3})\\s+from wallet\\s+(?<from_wallet>[0-9a-f-]+)\\s+at location\\s+(?<from_location>[^,]+),\\s+latitude\\s+(?<from_lat>[-\\d\\.]+),\\s+longitude\\s+(?<from_lon>[-\\d\\.]+)\\s+to\\s+(?<to_amount>\\d+)\\s+(?<to_currency>[A-Z]{3})\\s+in wallet\\s+(?<to_wallet>[0-9a-f-]+)\\s+at location\\s+(?<to_location>[^,]+),\\s+latitude\\s+(?<to_lat>[-\\d\\.]+),\\s+longitude\\s+(?<to_lon>[-\\d\\.]+)\"\n| eval amount=tonumber(amount), to_amount=tonumber(to_amount)\n| geostats latfield=from_lat longfield=from_lon count by currency",
        "queryParameters": {
          "earliest": "-24h@h",
          "latest": "now",
          "sampleRatio": 1
        }
      },
      "name": "Wallet Transfer Source Map - Cluster map search"
    },
    "ds_0hOxqJ58": {
      "type": "ds.search",
      "options": {
        "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" earliest=-24h latest=now (Deposited OR Withdrew)\n| rex field=_raw \"(?<action>Deposited|Withdrew)\\s+(?<amount>\\d+)\\s+(?<currency>[A-Z]{3})\\s+(?:to|from)\\s+wallet\\s+(?<wallet>[0-9a-f-]+)\\s+at location\\s+(?<location>[^,]+)\\s*,\\s*latitude\\s+(?<lat>[-\\d\\.]+),\\s*longitude\\s+(?<lon>[-\\d\\.]+)\"\n| eval amount=tonumber(amount)\n| stats count sum(amount) AS total_amount by action currency",
